{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/chaosarchives/src",
  "monorepo": true,
  "root": "apps/chaosarchives",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/chaosarchives/tsconfig.app.json",
    "assets": [
      "templates/**"
    ],
    "watchAssets": true
  },
  "generateOptions": {
    "spec": false
  },
  "projects": {
    "chaosarchives": {
      "type": "application",
      "root": "apps/chaosarchives",
      "entryFile": "main",
      "sourceRoot": "apps/chaosarchives/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chaosarchives/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "library",
      "root": "libs/authentication",
      "entryFile": "index",
      "sourceRoot": "libs/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "libs/authentication/tsconfig.lib.json"
      }
    },
    "authorization": {
      "type": "library",
      "root": "libs/authorization",
      "entryFile": "index",
      "sourceRoot": "libs/authorization/src",
      "compilerOptions": {
        "tsConfigPath": "libs/authorization/tsconfig.lib.json"
      }
    },
    "configuration": {
      "type": "library",
      "root": "libs/configuration",
      "entryFile": "index",
      "sourceRoot": "libs/configuration/src",
      "compilerOptions": {
        "tsConfigPath": "libs/configuration/tsconfig.lib.json"
      }
    },
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "security": {
      "type": "library",
      "root": "libs/security",
      "entryFile": "index",
      "sourceRoot": "libs/security/src",
      "compilerOptions": {
        "tsConfigPath": "libs/security/tsconfig.lib.json"
      }
    },
    "steward": {
      "type": "application",
      "root": "apps/steward",
      "entryFile": "main",
      "sourceRoot": "apps/steward/src",
      "compilerOptions": {
        "tsConfigPath": "apps/steward/tsconfig.app.json"
      }
    }
  }
}
